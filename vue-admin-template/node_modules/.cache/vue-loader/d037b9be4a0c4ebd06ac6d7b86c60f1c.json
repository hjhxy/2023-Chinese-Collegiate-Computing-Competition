ible\"\n      width=\"30%\"\n      center\n      :show-close=\"false\"\n    >\n      <div v-for=\"(item,index) in status\" :key=\"index\" class=\"mode\" @click=\"chooseMode(item)\">{{ item }}</div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport screenfull from 'screenfull'\nexport default {\n  name: 'Index',\n  data() {\n    return {\n      centerDialogVisible: false,\n      status: {\n        photo: '图像识别',\n        video: '视频识别',\n        camera: '摄像头识别'\n      },\n      currentStatus: '图像识别'\n    }\n  },\n  mounted() {\n    console.log(1)\n  },\n  methods: {\n    chooseMode(item) {\n      console.log(item)\n      const { photo, video, camera } = this.status\n      switch (item) {\n        case photo:\n          break\n        case video:\n          break\n        case camera:\n          break\n        default:\n          break\n      }\n    },\n    screenFull() {\n      if (!screenfull.isFullscreen && screenfull.isEnabled) {\n        screenfull.request()\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.dashboard {\n  &-container {\n    margin: 30px;\n\n    .header {\n      display: flex;\n      justify-content: flex-end;\n      align-items: center;\n      .choose_button{\n        width: fit-content;\n        margin-left: 20px;\n        padding: 10px 20px;\n        border-radius: 5px;\n        background-color: #0173e7;\n        color: #fff;\n        &:hover {\n          background-color: #3898fc;\n          cursor: pointer;\n        }\n      }\n    }\n  }\n  &-text {\n    font-size: 30px;\n    line-height: 46px;\n  }\n}\n\n::v-deep .el-dialog {\n  .mode {\n    margin: 5px 0px;\n    padding: 15px;\n    border-radius: 5px;\n    text-align: center;\n    background-color: #0173e7;\n    color: #fff;\n    &:hover{\n      cursor: pointer;\n      background-color: #3898fc;\n    }\n  }\n}\n</style>\n"]}]}